                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
                <structure>
                    <property name="position">2</property>
                    <property name="name">UNNAMED_2</property>
                    <property name="nativeName">applyLeft</property>
                    <property name="dataType">decimal</property>
                    <property name="nativeDataType">3</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT d.department_name as depName,
       ifnull(sum(rp.recp_number_expect)-sum(ra.reca_id),sum(rp.recp_number_expect)) as applyLeft
FROM recruitplan rp left join
(select * from recruitapplier where reca_status =13) ra on rp.recp_id=ra.reca_id , department d
where rp.recp_status=12 and rp.recp_department_no=d.department_no group by depName</property>
        </oda-data-set>

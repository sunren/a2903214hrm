                    <property name="name">UNNAMED_2</property>
                    <property name="nativeName">empExpect</property>
                    <property name="dataType">decimal</property>
                    <property name="nativeDataType">3</property>
                </structure>
                <structure>
                    <property name="position">3</property>
                    <property name="name">UNNAMED_3</property>
                    <property name="nativeName">applyNum</property>
                    <property name="dataType">decimal</property>
                    <property name="nativeDataType">3</property>
                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">UNNAMED_4</property>
                    <property name="nativeName">applyLeft</property>
                    <property name="dataType">decimal</property>
                    <property name="nativeDataType">3</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT count(rp.recp_id) as rpNum,
       sum(rp.recp_number_expect) as empExpect,
       ifnull(sum(ra.reca_id),0) as applyNum,
       ifnull(sum(rp.recp_number_expect)-sum(ra.reca_id),sum(rp.recp_number_expect)) as applyLeft
FROM recruitplan rp left join
(select * from recruitapplier where reca_status =13) ra on rp.recp_id=ra.reca_id
where rp.recp_status=12</property>
        </oda-data-set>

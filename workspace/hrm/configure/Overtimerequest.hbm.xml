<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.hr.examin.domain">
	<class
		name="Overtimerequest"
		table="overtimerequest"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>   
		<id
			name="orId"
			type="string"
			column="or_id"
		>
			<generator class="assigned"/>
		</id>
		<property
			name="orNo"
			column="or_no"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="orReason"
			column="or_reason"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="orStartDate"
			column="or_start_date"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="orEndDate"
			column="or_end_date"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="orTotalHours"
			column="or_total_hours"
			type="big_decimal"
			not-null="true"
			length="7"
		/>
		<property
			name="orTiaoxiuHours"
			column="or_tiaoxiu_hours"
			type="big_decimal"
			not-null="false"
			length="7"
		/>
		<property
			name="orTiaoxiuExpire"
			column="or_tiaoxiu_expire"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="orNextApprover"
			column="or_next_approver"
			type="string"
			not-null="false"
			length="36"
		/>
		<property
			name="orStatus"
			column="or_status"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="orCreateTime"
			column="or_create_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="orLastChangeTime"
			column="or_last_change_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="orSecurityNo"
			column="or_security_no"
			type="string"
			length="36"
		/>
		<many-to-one
			name="orCreateBy"
			column="or_create_by"
			class="com.hr.profile.domain.Employee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="orOtNo"
			column="or_ot_no"
			class="Overtimetype"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="orLastChangeBy"
			column="or_last_change_by"
			class="com.hr.profile.domain.Employee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="orEmpDept"
			column="or_emp_dept"
			class="com.hr.configuration.domain.Department"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="orEmpNo"
			column="or_emp_no"
			class="com.hr.profile.domain.Employee"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="orEmpLocationNo"
			column="or_emp_location"
			class="com.hr.configuration.domain.Location"
			not-null="true"
		>
		</many-to-one>

	</class>	
</hibernate-mapping>
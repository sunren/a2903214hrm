<?xml version="1.0"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="report.reportName">
		<field-validator type="required">
			<message>必填项！</message>
		</field-validator>
	</field>
	<field name="report.reportModule">
		<field-validator type="required">
			<message>必填项！</message>
		</field-validator>
	</field>
	<field name="report.reportAuthority">
		<field-validator type="required">
			<message>必填项！</message>
		</field-validator>
	</field>
	<field name="report.reportUrl">
		<field-validator type="required">
			<message>必填项！</message>
		</field-validator>
	</field>
	<field name="reportFile">
		<field-validator type="required">
			<message>必填项！</message>
		</field-validator>
	</field>
	<field name="report.reportSortId">
		<field-validator type="required">
			<message>必填项！</message>
		</field-validator>
		<field-validator type="int">
			<message>格式错误！</message>
		</field-validator>
	</field>
</validators>